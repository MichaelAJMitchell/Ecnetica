  "nodes": [
    {
      "id": "c13f817d-8ae8-4a41-96b0-58cc660d0aa3",
      "label": "Pythagorean Identity",
      "group": "Trigonometry",
      "title": "The fundamental trigonometric identity: cos²A + sin²A = 1.",
      "broader_concept": "Trigonometric Identities",
      "grade_level": "High",
      "difficulty": "Basic"
    },
    {
      "id": "0d9fed40-5e17-4c40-97c3-73eb6503deba",
      "label": "Sine Rule",
      "group": "Trigonometry",
      "title": "A formula relating the sides and angles of a triangle: a/sinA = b/sinB = c/sinC.",
      "broader_concept": "Trigonometric Laws",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "a5d18d0d-73a1-449c-9509-712861b32204",
      "label": "Cosine Rule",
      "group": "Trigonometry",
      "title": "A formula relating the lengths of sides of a triangle to the cosine of one of its angles: a² = b² + c² – 2bc cosA.",
      "broader_concept": "Trigonometric Laws",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "19fc9cea-3780-4efb-a8f3-c1ca6dc487c3",
      "label": "Cosine of Sum Formula",
      "group": "Trigonometry",
      "title": "Formula for the cosine of the sum of two angles: cos(A+B) = cosA cosB – sinA sinB.",
      "broader_concept": "Angle Addition and Subtraction Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "1f5400a1-61f5-4636-8aa0-6556b3a87b6e",
      "label": "Cosine of Difference Formula",
      "group": "Trigonometry",
      "title": "Formula for the cosine of the difference of two angles: cos(A–B) = cosA cosB + sinA sinB.",
      "broader_concept": "Angle Addition and Subtraction Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "5c26bed7-e029-4a82-872c-c494c7df4136",
      "label": "Double Angle Formula for Cosine",
      "group": "Trigonometry",
      "title": "Formula for the cosine of double an angle: cos2A = cos²A – sin²A.",
      "broader_concept": "Double Angle Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "cbd7205c-3584-4431-8dff-2ba4a918ae8d",
      "label": "Sine of Sum Formula",
      "group": "Trigonometry",
      "title": "Formula for the sine of the sum of two angles: sin(A+B) = sinA cosB + cosA sinB.",
      "broader_concept": "Angle Addition and Subtraction Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "90a741d9-7b7b-4f0c-8393-9de0d75e51be",
      "label": "Sine of Difference Formula",
      "group": "Trigonometry",
      "title": "Formula for the sine of the difference of two angles: sin(A–B) = sinA cosB – cosA sinB.",
      "broader_concept": "Angle Addition and Subtraction Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "28c5cee3-cfb7-493a-b689-8d84cf1adc4c",
      "label": "Tangent of Sum Formula",
      "group": "Trigonometry",
      "title": "Formula for the tangent of the sum of two angles: tan(A+B) = (tanA + tanB) / (1 – tanA tanB).",
      "broader_concept": "Angle Addition and Subtraction Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "0e80fff8-9886-446b-9bca-262c97d69f9c",
      "label": "Tangent of Difference Formula",
      "group": "Trigonometry",
      "title": "Formula for the tangent of the difference of two angles: tan(A–B) = (tanA – tanB) / (1 + tanA tanB).",
      "broader_concept": "Angle Addition and Subtraction Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "d2574169-f40c-43e3-8016-83a3721f11ad",
      "label": "Double Angle Formula for Sine",
      "group": "Trigonometry",
      "title": "Formula for the sine of double an angle: sin2A = 2 sinA cosA.",
      "broader_concept": "Double Angle Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "edece443-7531-40e1-bd9b-bd067c40064f",
      "label": "Double Angle Formula for Tangent",
      "group": "Trigonometry",
      "title": "Formula for the tangent of double an angle: tan2A = 2 tanA / (1 – tan²A).",
      "broader_concept": "Double Angle Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
    {
      "id": "43d979a8-e87c-49fb-b430-959046b6e5f2",
      "label": "Alternative Double Angle Formulas for Sine and Cosine",
      "group": "Trigonometry",
      "title": "Alternative forms for sin2A and cos2A using only sine or cosine: sin2A = 2 tanA / (1 + tan²A), cos2A = (1 – tan²A) / (1 + tan²A).",
      "broader_concept": "Double Angle Formulas",
      "grade_level": "High",
      "difficulty": "Intermediate"
    },
